{% extends "layout.twig" %}

{% block content %}

<div class="template-feature">
	{{widget('feature', 'template-feature', {"header": "profile", "strapline":"profile", "buttonText":"Contact us","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true})|raw}}
</div>

<main class="template-main">

	<section class="main-content wrapper clearfix">
		<div class="responsivecolumns columns-two-66-34">
			<div class="column columns-two-left">
				{{zone('main')|raw}}
			</div>
			<div id="div-form" class="column columns-two-right">
				{{zone('main')|raw}}
			</div>
		</div>
	</section>
	
</main>

{% endblock %}